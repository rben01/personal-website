		"adoc",
		"antora",
		"formatter's",
		"Gofmt",
		"Iosevka",
		"linenums",
		"lunr",
		"monospace",
		"rltb",
		"Rossum",
		"Rustfmt",
		"Simplicio",
		"Simplicio's",
		"tok",
		"unindented",
		"xyz"
	]
}
