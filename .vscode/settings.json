{
	"spellright.language": [
		"en"
	],
	"spellright.documentTypes": [
		"asciidoc",
		"markdown",
		"latex",
		"plaintext",
		"html"
	],
	"liveServer.settings.port": 5502,
	"liveServer.settings.wait": 400,
	"code-runner.executorMap": {
		"asciidoc": "asciidoctor -r ./syntax-highlighter $dir$fileName",
	},
